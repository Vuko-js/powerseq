{
  "name": "powerseq",
  "version": "0.0.8",
  "description": "Sequence operators",
  "scripts": {
    "del": "npm-run-all del-dist del-npmpackage",
    "del-dist": "shx rm -rf ./dist",
    "del-npmpackage": "shx rm -rf ./npmpackage",
    "tsc": "node tools/tsc.js",
    "gen-methods": "node tools/genMethods.js",
    "gen-package-json": "node tools/genPackageJson.js",
    "gen-readme-md": "node tools/genReadme.js",
    "watch": "node tools/tsc.js watch",
    "test": "mocha ./dist/test -R spec --recursive --timeout 30000",
    "rebuild": "npm-run-all del gen-methods tsc",
    "copy-src-npmpackage": "mkdirp ./npmpackage/src/ && shx cp -r ./src/* ./npmpackage/src",
    "copy-compiled-src-npmpackage": "mkdirp ./npmpackage && shx cp -r ./dist/src/* ./npmpackage",
    "copy-esm-src-npmpackage": "mkdirp ./npmpackage/esm && shx cp -r ./dist/esm/src/* ./npmpackage/esm",
    "tsc-esm": "./node_modules/.bin/tsc -p ./tsconfig.esm.json",
    "tsc-esm-nohelpers": "./node_modules/.bin/tsc -p ./tsconfig.esm.nohelpers.json",         
    "bundles": "npm run tsc-esm-nohelpers && ./node_modules/.bin/mkdirp ./npmpackage/bundles && node ./tools/make-umd-bundle.js",
    "build-npmpackage": "npm-run-all rebuild tsc-esm copy-src-npmpackage copy-compiled-src-npmpackage copy-esm-src-npmpackage gen-readme-md gen-package-json bundles" 
  },
  "dependencies": {},
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/mocha": "^2.2.33",
    "@types/node": "0.0.2",
    "glob": "^7.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "npm-scripts-info": "^0.3.6",
    "rollup": "^0.37.0",
    "shx": "^0.2.1",
    "tslib": "1.7.0",
    "typescript": "2.3.0"
  },
  "author": "Marcin Najder <marcin.najder@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.8.1"
  },
  "bugs": {
    "url": "https://github.com/marcinnajder/powerseq/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:marcinnajder/powerseq.git"
  }
}
